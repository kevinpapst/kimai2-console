{
    "name": "kevinpapst/kimai2-console",
    "license": "MIT",
    "type": "project",
    "description": "Kimai 2 - console application to manage your time-tracking data remotely",
    "authors": [
        {
            "name": "Kevin Papst",
            "homepage": "https://www.kevinpapst.de"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-iconv": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5",
        "symfony/console": "5.0.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "KimaiConsole\\": "src/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
